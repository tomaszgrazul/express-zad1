<div class="container text-center">
  <div class="row">
    <div class="col">
      <h2>Name and surname</h2>
    </div>
    <div class="col">
      <h2>Address</h2>
    </div>
    <div class="col">
      <H2>Website</H2>
    </div>
  </div>
</div>

{{#each user}}
<div class="container text-center">
  <div class="row">
    <div class="col my">
      {{name}}
    </div>
    <div class="col my">
      {{address.street}}
      {{address.suite}}
      {{address.city}}
      {{address.zipcode}}
    </div>
    <div class="col my">
      {{website}}
    </div>
  </div>
</div>
{{/each}}